<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="246.0" prefWidth="572.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dnd.CharCreation.ClassMenus.BarbarianSkillsMenuController">
    <children>
      <Label alignment="CENTER" layoutX="-1.0" layoutY="14.0" prefHeight="35.0" prefWidth="572.0" text="Barbarian Skills">
         <font>
            <Font name="Copperplate Gothic Light" size="31.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutY="59.0" prefHeight="17.0" prefWidth="572.0" text="Choose two of the skills below to become proficient in" />
      <Button fx:id="nextBut" disable="true" layoutX="472.0" layoutY="190.0" mnemonicParsing="false" onAction="#nextButton" prefHeight="34.0" prefWidth="80.0" text="Next">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button fx:id="backBut" layoutX="378.0" layoutY="190.0" mnemonicParsing="false" onAction="#backButton" prefHeight="34.0" prefWidth="80.0" text="Back">
         <font>
            <Font size="15.0" />
         </font></Button>
      <GridPane layoutX="88.0" layoutY="93.0" prefHeight="60.0" prefWidth="451.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <CheckBox fx:id="animalCB" mnemonicParsing="false" text="Animal Handling" />
            <CheckBox fx:id="intimidationCB" mnemonicParsing="false" text="Intimidation" GridPane.columnIndex="1">
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font></CheckBox>
            <CheckBox fx:id="perceptionCB" mnemonicParsing="false" text="Perception" GridPane.columnIndex="2">
               <font>
                  <Font size="15.0" />
               </font></CheckBox>
            <CheckBox fx:id="athleticsCB" mnemonicParsing="false" text="Athletics" GridPane.rowIndex="1">
               <font>
                  <Font name="Segoe UI" size="15.0" />
               </font></CheckBox>
            <CheckBox fx:id="natureCB" mnemonicParsing="false" text="Nature" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="10.0" />
               </font></CheckBox>
            <CheckBox fx:id="survivalCB" mnemonicParsing="false" text="Survival" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <font>
                  <Font name="Segoe UI" size="10.0" />
               </font></CheckBox>
         </children>
      </GridPane>
   </children>
